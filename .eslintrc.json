{
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 2018,
		"sourceType": "module"		
	},
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"plugin:vue/recommended",
		"plugin:prettier/recommended",
		"prettier/vue"
	],
	"rules": {
		"no-await-in-loop": 0,
		"no-plusplus": 0,
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
		"vue/require-default-prop": false,
		"class-methods-use-this": 0,
		"no-param-reassign": 0,
		"no-underscore-dangle": 0
	}
}